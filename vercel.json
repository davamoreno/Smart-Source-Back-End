{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/svg/(.*)",
      "dest": "/public/svg/$1"
    }],
  "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "true",
      "APP_URL": "https://smart-source-project.vercel.app",
      "APP_KEY": "base64:WdZ2nSfdT2UrWXwo9PbFPSYgCcFcjH7g4lj+WEk4mnQ=",

      "APP_CONFIG_CACHE": "/tmp/config.php",
      "APP_EVENTS_CACHE": "/tmp/events.php",
      "APP_PACKAGES_CACHE": "/tmp/packages.php",
      "APP_ROUTES_CACHE": "/tmp/routes.php",
      "APP_SERVICES_CACHE": "/tmp/services.php",
      "VIEW_COMPILED_PATH": "/tmp",

      "CACHE_DRIVER": "array",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie"
  }
}