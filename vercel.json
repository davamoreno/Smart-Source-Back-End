{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "build": {
      "env": {
        "NODE_ENV": "production"
      } 
    },
      "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.php"
      }
    ]
}