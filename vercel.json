{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "build": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "npm run build && php artisan optimize:clear" 
    },
      "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.php"
      }
    ]
}